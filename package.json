    "terser": "^5.17.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "is-unicode-supported": "^1.3.0",
    "unbuild": "^1.2.1",
    "wrap-ansi": "^8.1.0"
  },
  "keywords": [
    "CLI",
    "Prompt"
  ],
  "bugs": {
    "url": "https://github.com/unprompts/unprompts/issues"
  },
  "homepage": "https://github.com/unprompts/unprompts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unprompts/unprompts.git"
